            <dxlc:LayoutGroup>
                <dxlc:LayoutGroup Header="{Binding BoxZone}" Height="Auto" View="GroupBox" Orientation="Vertical">
                    <dxlc:DataLayoutItem>
                        <Grid Margin="5">
